@using relmon.Utilities;
@using relmon.Models;
    @( Html.Telerik().TabStrip()
        .Name("TabStrip")
        .Items(parent => {
            int aclId = (int)Session["id"];
            if (ACLHandler.isUserAllowedTo(PageItem.DataBisnis_Ap_Profil.name + ViewBag.id, aclId, "view"))
            {
                parent.Add()
                    .Text("Profil")
                    .LoadContentFrom("Profil/" + ViewBag.id, "DataBisnisAp")
                    .Selected(true);
            }
            if (ACLHandler.isUserAllowedTo(PageItem.DataBisnis_Ap_StrukturOrganisasi.name+ ViewBag.id, aclId, "view"))
            {
                parent.Add()
                .Text("Struktur Organisasi")
                .LoadContentFrom("StrukturOrganisasi/" + ViewBag.id, "DataBisnisAp");
             }
            if (ACLHandler.isUserAllowedTo(PageItem.DataBisnis_Ap_ADART.name + ViewBag.id, aclId, "view"))
            {
                parent.Add()
                .Text("AD/ART")
                .LoadContentFrom("AdArt/"+ViewBag.id, "DataBisnisDitGas");
             }
            //if (ACLHandler.isUserAllowedTo(PageItem..name + ViewBag.id, aclId, "view"))
            if(true)
            {
                parent.Add()
                .Text("Visi Misi")
                .LoadContentFrom("RencanaKerja/" + ViewBag.id, "DataBisnisAp");
             }
            if (ACLHandler.isUserAllowedTo(PageItem.DataBisnis_Ap_RJPP.name + ViewBag.id, aclId, "view"))
            {
                parent.Add()
                .Text("RJPP")
                .LoadContentFrom("Rjpp/" + ViewBag.id, "DataBisnisDitGas");
            }
            if (ACLHandler.isUserAllowedTo(PageItem.DataBisnis_Ap_RKAP.name + ViewBag.id, aclId, "view"))
            {
                parent.Add()
                .Text("RKAP")
                .LoadContentFrom("Rkap/" + ViewBag.id, "DataBisnisDitGas");
            }
            if (ACLHandler.isUserAllowedTo(PageItem.DataBisnis_Ap_BussinessReport.name + ViewBag.id, aclId, "view"))
            {
                if (ViewBag.id == 2 || ViewBag.id == 3 || ViewBag.id == 4)
                {
                    parent.Add()
                    .Text("Bussiness Report")
                    .LoadContentFrom("BussinessReport/" + ViewBag.id, "DataBisnisDitGas");
                }
            }
            if (ACLHandler.isUserAllowedTo(PageItem.DataBisnis_Ap_KPI.name + ViewBag.id, aclId, "view"))
            {
                if (ViewBag.id == 1 || ViewBag.id == 9)
                {

                    parent.Add()
                    .Text("KPI")
                    .LoadContentFrom("Kinerja/" + ViewBag.id, "DataBisnisDitGas");
                }
            }
            //if (ACLHandler.isUserAllowedTo(PageItem.DataBisnis_Ap_ProjectStatus.name + ViewBag.id, aclId, "view"))
            //{
            //    parent.Add()
            //    .Text("Project Status")
            //    .LoadContentFrom("ProjectStatus/" + ViewBag.id, "DataBisnisDitGas");
            // }
            if (ACLHandler.isUserAllowedTo(PageItem.DataBisnis_Ap_Product.name + ViewBag.id, aclId, "view"))
            {
                parent.Add()
                   .Text("Product")
                   .LoadContentFrom("ProductNavigator/" + ViewBag.id, "DataBisnisAp");
            }
        })
    )
