<style>
    #VisiMisi li
    {
        background-color:#6d6e72 !important;
    }
</style>

@(Html.Telerik().PanelBar()
    .Name("VisiMisi")
    .HtmlAttributes(new { style = "width: 900px; margin-bottom: 10px;" })
    .Items(panelBarItem =>
    {
        panelBarItem.Add()
            .Text("Visi Misi Direktorat Gas")
            .Content(@<text>
                <iframe id="frame-content" name="frame-content" src="/DirektoratDitGas/getPDFVisiMisi/" width="900" height="600" style="border:0px solid #000;"></iframe>
                </text>)
            .Expanded(false);
    })
        .ExpandMode(PanelBarExpandMode.Multiple)
    )
<br />
<hr />
@using (Html.BeginForm("uploadVisiMisi", "DashboardDefault", FormMethod.Post, new { id = "formUpload", enctype = "multipart/form-data", name = "formUpload", target = "refresh_targetVisiMisi" }))
{
    <div class="info_inside_left">Upload Visi Misi (.pdf) :</div> 
		 <input type="text" id="fileName" class="file_input_textbox" readonly="readonly" />
		 <div class="file_input_div">
			<input type="button" value="Cari" class="file_input_button" />
			<input type="file" class="file_input_hidden" name="userfile" onchange="javascript: document.getElementById('fileName').value = this.value" />
		 </div>
		 <br />
		 <input type="button" value="Upload" onclick="javascript:if(document.formUpload.userfile.value.length==0) { alert('You must specify a file first'); return; }; submit(); hideVisiMisi()" />
}